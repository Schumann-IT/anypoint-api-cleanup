{
    "type": "object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "required": true,
    "properties": {
      "groupId": {
        "type": "string",
        "maxLength": 256,
        "minLength": 1,
        "required": true
      },
      "assetId": {
        "type": "string",
        "maxLength": 256,
        "minLength": 1,
        "required": true
      },
      "assetVersion":{
        "type": "string",
        "maxLength": 256,
        "minLength": 1,
        "required": true
      },
      "configurationData": {
        "type": ["object", "array"],
        "required": false
      },
      "order": {
        "type": "integer",
        "minimum": 1
      }
    }
}
