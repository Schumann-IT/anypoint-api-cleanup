{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "properties": {
    "managedServiceApiId": {
      "type": "integer",
      "minimum": 1
    },
    "name": {
      "type": "string",
      "maxLength": 256,
      "minLength": 1
    },
    "description":{
      "type": ["string", "null"],
      "maxLength": 256,
      "minLength": 1
    },
    "autoApprove": {
      "type": "boolean"
    },
    "status": {
      "type": "string",
      "enum": [
        "ACTIVE",
        "DEPRECATED"
      ]
    },
    "limits": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "maximumRequests",
          "timePeriodInMilliseconds",
          "visible"
        ],
        "properties": {
          "maximumRequests": {
            "type": "integer",
            "minimum": 1
          },
          "timePeriodInMilliseconds": {
            "type": "integer",
            "minimum": 1
          },
          "visible": {
            "type": "boolean"
          }
        }
      }
    }
  }
}
