{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "masterOrganizationId": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12}$"
    },
    "applicationId": {
      "type": "integer"
    },
    "acceptedTerms":{
      "type": "boolean"
    },
    "apiId": {
      "type": "string"
    },
    "environmentId": {
      "type": ["string", "null"]
    },
    "requestedTierId": {
      "type": ["integer", "null"]
    },
    "organizationId":{
      "type": "string",
      "maxLength": 36,
      "pattern": "^[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}$"
    },
    "groupId": {
      "type": "string"
    },
    "assetId": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "productAPIVersion":{
      "type": "string",
      "maxLength": 128
    },
    "versionGroup":{
      "type": "string",
      "maxLength": 128
    }
  },
  "anyOf": [
    { "required": ["productAPIVersion"] },
    { "required": ["versionGroup"] }
  ]
}
